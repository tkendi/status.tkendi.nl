{
  "name": "status",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn env:copy && yarn graphql:codegen && next dev",
    "build": "yarn env:copy && yarn graphql:codegen && next build",
    "env:copy": "cp .env .env.local",
    "graphql": "DOTENV_CONFIG_PATH=.env.local graphql-let -r dotenv/config",
    "graphql:codegen": "DOTENV_CONFIG_PATH=.env.local graphql-codegen -r dotenv/config",
    "vercel": "pnpm vercel:link && vercel --cwd ../..",
    "vercel:link": "cd apps/status && mkdir -p .git && touch .git/config && vercel link --scope tkendi --yes && cp -R .vercel ../../../..",
    "start": "next start",
    "lint": "next lint --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@babel/generator": "^7.19.3",
    "@babel/types": "^7.19.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@next/font": "^13.1.1",
    "@nivo/axes": "^0.80.0",
    "@nivo/colors": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/legends": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "@nivo/tooltip": "^0.80.0",
    "@react-stately/collections": "^3.4.4",
    "@react-stately/menu": "^3.4.2",
    "@stitches/react": "^1.2.8",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.191",
    "axios": "^1.4.0",
    "cross-fetch": "^3.1.5",
    "d3-scale": "^4.0.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "next": "^14.0.2",
    "next-themes": "^0.2.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-icons": "^4.8.0",
    "ts-node": "^10.9.1",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.3",
    "@as-integrations/next": "^1.2.0",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/import-types-preset": "^2.2.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@graphql-tools/merge": "^8.4.0",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@yarnpkg/sdks": "^3.0.0-rc.34",
    "eslint": "8.24.0",
    "eslint-config-custom": "workspace: *",
    "graphql": "^16.6.0",
    "graphql-let": "^0.18.6",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "import": "^0.0.6",
    "prettier": "^2.8.3",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@4.0.0-rc.22"
}
